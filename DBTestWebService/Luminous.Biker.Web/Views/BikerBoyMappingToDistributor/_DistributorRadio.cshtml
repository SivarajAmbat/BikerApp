@using Luminous.Biker.Web.Models;
@{
    LuminousBikerAppEntities db = new LuminousBikerAppEntities();
    List<DistDealerDetail> distDealerDetails = db.DistDealerDetails.ToList();


    if (distDealerDetails != null && distDealerDetails.Count > 0)
    {
        foreach (var item in distDealerDetails)
        {
            <div class="chbDistMain">
                @Html.Hidden("hndDistId", item.ID)
                @Html.RadioButton("rdbDistId", "", false, new { @class = "rdbDistId" }) &nbsp; @Html.Label("lblDistId", item.Dis_Sap_Code + item.Dis_Name)
            </div>
        }
    }
}
