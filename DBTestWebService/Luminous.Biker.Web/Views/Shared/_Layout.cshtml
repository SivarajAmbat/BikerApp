<!DOCTYPE html>
<html lang="en">
<head>
    <meta http-equiv="Content-Type" content="text/html; charset=UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <!-- The above 3 meta tags *must* come first in the head; any other head content must come *after* these tags -->
    <meta name="description" content="">
    <meta name="author" content="">
    <link rel="icon" href="http://getbootstrap.com/favicon.ico">
    <title>Biker App</title>
    <!-- Bootstrap core CSS -->
    <link href="@Url.Content("~/App_Themes/Responsive/css/bootstrap.min.css")" rel="stylesheet">
    <!-- IE10 viewport hack for Surface/desktop Windows 8 bug -->
    <link href="@Url.Content("~/App_Themes/Responsive/css/ie10-viewport-bug-workaround.css")" rel="stylesheet">
    <!-- Custom styles for this template -->
    <link href="@Url.Content("~/Style/blog.css")" rel="stylesheet">
    <!-- Just for debugging purposes. Don't actually copy these 2 lines! -->
    <!--[if lt IE 9]><script src="../../assets/js/ie8-responsive-file-warning.js"></script><![endif]-->
    <script src="@Url.Content("~/App_Themes/Responsive/js/ie-emulation-modes-warning.js")"></script>
    <!-- HTML5 shim and Respond.js for IE8 support of HTML5 elements and media queries -->
    <!--[if lt IE 9]>
      <script src="https://oss.maxcdn.com/html5shiv/3.7.2/html5shiv.min.js"></script>
      <script src="https://oss.maxcdn.com/respond/1.4.2/respond.min.js"></script>
    <![endif]-->
</head>
<body>
    <div class="blog-masthead">
        <div class="container-fluid">
            <nav class="blog-nav">
                @Html.ActionLink("Home", "Index", "Home", null, new { @class = "blog-nav-item" })
                @Html.ActionLink("All Distributors", "Index", "CDistributor", null, new { @class = "blog-nav-item" })
                @Html.ActionLink("All Biker Boys", "Index", "CBikerBoy", null, new { @class = "blog-nav-item" })
                @Html.ActionLink("All Beats", "Index", "CBeat", null, new { @class = "blog-nav-item" })
                @Html.ActionLink("All Retailers", "Index", "CRetailer", null, new { @class = "blog-nav-item" })
                @Html.ActionLink("All Items", "Index", "CItem", null, new { @class = "blog-nav-item" })
                @Html.ActionLink("Map Biker Boy to Distributor", "Index", "BikerBoyMappingToDistributor", null, new { @class = "blog-nav-item" })
                @*@Html.ActionLink("Map Beat to Distributor", "Index", "BeatMappintToDistributor", null, new { @class = "blog-nav-item" })*@
                @*@Html.ActionLink("Map Retailer to Beat", "Index", "RetailerToBeatMapping", null, new { @class = "blog-nav-item" })*@
                @Html.ActionLink("Logs", "Index", "Logger", null, new { @class = "blog-nav-item" })
                <p class="pull-right" style="margin:0px;">
                    <a class="blog-nav-item" href="javascript:$('#logOffForm').submit()" @(ViewBag.Title == "Login" ? "style=display:none;" : "")>Log Off</a>
                </p>
            </nav>
        </div>
    </div>
    <div class="container-fluid">
        @*<div class="blog-header">
            <h3 class="blog-title">Biker Master</h3>
            <p class="lead blog-description">The official example template of creating a blog with Bootstrap.</p>
        </div>*@
        <div class="row">
            <div class="col-sm-12 col-md-12 col-lg-12 col-xs-12 blog-main">
                @RenderBody()
            </div><!-- /.blog-main -->
            @*<div class="col-sm-12 col-md-2 col-lg-2 col-xs-2 col-md-offset-1 col-xs-offset-1 col-lg-offset-1 blog-sidebar">
                @{Html.RenderPartial("~/Views/Shared/_MenuItems.cshtml");}
            </div>*@<!-- /.blog-sidebar -->
        </div><!-- /.row -->
    </div><!-- /.container -->
    <footer class="blog-footer">
        <p>@Html.ActionLink("Home", "Index", "Home").</p>
        <p>
            @Html.ActionLink("Back to Top", "Index", "Home")
        </p>
    </footer>

    <!-- Bootstrap core JavaScript
    ================================================== -->
    <!-- Placed at the end of the document so the pages load faster -->
    <script src="@Url.Content("~/Script/jquery.min.js")"></script>
    @*<script>window.jQuery || document.write('<script src="../../assets/js/vendor/jquery.min.js"><\/script>')</script>*@
    <script src="@Url.Content("~/App_Themes/Responsive/js/bootstrap.min.js")"></script>
    <!-- IE10 viewport hack for Surface/desktop Windows 8 bug -->
    <script src="@Url.Content("~/App_Themes/Responsive/js/ie10-viewport-bug-workaround.js")"></script>
    @using (Html.BeginForm("LogOff", "Account", FormMethod.Post, new { id = "logOffForm" }))
    {
        @Html.AntiForgeryToken()
    }
</body>
</html>