@model Luminous.Biker.Web.Models.BikerDistMapping

@{
    ViewBag.Title = "Create";
}

<h2  class="col-lg-12 col-xs-12 col-md-12 col-sm-12">Create</h2>

@using (Html.BeginForm()) {
    @Html.AntiForgeryToken()
    @Html.ValidationSummary(true)

    <fieldset class="col-lg-12 col-xs-12 col-md-12 col-sm-12">
        <legend>Biker Distributor Mapping</legend>

        <div class="editor-label col-md-2">
            @Html.LabelFor(model => model.Distributor_Id, "DistDealerDetail")
        </div>
        <div class="editor-field col-md-4">
            @Html.DropDownList("Distributor_Id", String.Empty)
            @Html.ValidationMessageFor(model => model.Distributor_Id)
        </div>

        <div class="editor-label col-md-2">
            @Html.LabelFor(model => model.BikerBoy_Id, "BikerBoyDetail")
        </div>
        <div class="editor-field col-md-4">
            @Html.DropDownList("BikerBoy_Id", String.Empty)
            @Html.ValidationMessageFor(model => model.BikerBoy_Id)
        </div>

        <p>
            <input type="submit" class="btn btn-primary" value="Create" />
        </p>
    </fieldset>
}

<div class="col-lg-12 col-xs-12 col-md-12 col-sm-12">
    <p>@Html.ActionLink("Back to List", "Index")</p>
</div>


